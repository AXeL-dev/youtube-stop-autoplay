{
  "name": "Stop AutoPlay Next for YouTubeâ„¢",
  "short_name": "isafyoutube",
  "description": "Stop YouTube from Playing next tracks (aka disable autoplay button).",
  "author": "James Fray",
  "version": "0.1.4",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "*://*.youtube.com/yts/jsbin/*",
    "*://*.ytimg.com/yts/jsbin/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*",
        "http://*.youtube.com/*"
      ],
      "js": ["data/inject.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "data/options/index.html",
    "chrome_style": true
  },
  "homepage_url": "http://add0n.com/stop-autoplay.html",
  "icons": {
    "16": "data/icons/16.png",
    "48": "data/icons/48.png",
    "128": "data/icons/128.png"
  }
}
